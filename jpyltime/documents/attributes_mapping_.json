        }
    },
    "Postal code": {
        "fhir_resource": "Patient",
        "fhir_source": {
            "select": ["Patient.address[0].postalCode"]
        }
    },
    "ICD-9 Diagnosis": {
        "fhir_resource": "Condition",
        "fhir_source": {
            "select": ["Condition.code.coding.code", "Condition.code.coding.display"],
            "join": [{
                "key": "Patient.identifier[0].value",
                "value": "Condition.subject.identifier.value"
            }],
            "where": [{
                "key": "Condition.code",
                "value": "http://hl7.org/fhir/sid/icd-9%7C"
            }]
        }
    },
    "Medication": {
        "fhir_resource": "MedicationRequest",
        "fhir_source": {
            "select": ["MedicationRequest.medicationCodeableConcept.coding.display", "MedicationRequest.dosageInstruction.doseAndRate.doseQuantity.value", "MedicationRequest.dosageInstruction.doseAndRate.doseQuantity.unit"],
            "join": [{
                "key": "Patient.identifier[0].value",
                "value": "MedicationRequest.subject.identifier.value"
            }]
        }
    },
    "Weight": {
        "fhir_resource": "Observation as Weight",
        "fhir_source": {
            "select": ["Weight.valueQuantity", "Weight.valueQuantity.unit"],
            "join": [{
                "key": "Patient.identifier[0].value",
                "value": "Weight.subject.identifier.value"
            }],
            "where": [{
                "key": "Weight.code.coding[0].system",
                "value": "http://loinc.org"
            }, {
                "key": "Weight.code.coding[0].code",
                "value": "29463-7"
            }]
        }
    },
