{
    "Identifier": {
        "fhir_resource": "Patient",
        "fhir_source": {
            "select": ["Patient.identifier[0].value"]
        }
    },
    "Last name": {
        "fhir_resource": "Patient",
        "fhir_source": {
            "select": ["Patient.name[0].family"]
        }
    },
    "First name": {
        "fhir_resource": "Patient",
        "fhir_source": {
            "select": ["Patient.name[0].given[0]"]
        }
    },
    "Birthdate": {
        "fhir_resource": "Patient",
        "fhir_source": {
            "select": ["Patient.birthdate"]
        }
    },
    "Gender": {
        "fhir_resource": "Patient",
